# Vite Production Environment Variables
# These will be used when building for production

# API Configuration - Update this with your actual Render backend URL
VITE_API_BASE_URL=https://lane-google.onrender.com

# Application Settings
VITE_APP_NAME=Lane AI
VITE_APP_VERSION=2.0.0

# Feature Flags
VITE_FEATURE_AI_CHAT=true
VITE_FEATURE_REAL_TIME_MONITORING=true
VITE_FEATURE_AUTO_OPTIMIZATION=true
VITE_FEATURE_WORKFLOW_APPROVAL=true

# Google OAuth Configuration (if needed for frontend OAuth)
# This is a public key, safe for frontend
VITE_GOOGLE_OAUTH_CLIENT_ID=

# Default Settings
VITE_DEFAULT_CUSTOMER_ID=demo-customer
VITE_PAGINATION_SIZE=20
VITE_REFRESH_INTERVAL=30000
VITE_TOKEN_REFRESH_THRESHOLD=300000

# Environment Mode
VITE_ENVIRONMENT=production